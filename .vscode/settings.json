{
  "files.exclude": {
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/CVS": true,
    "**/.DS_Store": true,
    "**/Thumbs.db": true,
    "**/.wrangler": true,
    "**/migrations": true,
    "**/node_modules": true,
    "**/tsconfig.json": true,
    "**/wrangler.toml": true,
    "**/.prettierrc": true,
    "**/.watchmanconfig": true,
    "**/tsconfig.base.json": true,
    "**/.gitignore": true,
    "**/types": true,
    "**/dist": true,
    "apps/expo/index.js": true,
    "**/metro.config.js": true,
    "**/eas.json": true,
    "**/tamagui.config.ts": true,
    "**/babel.config.js": true,
    "**/.expo": true,
    "**/.next": true,
    "**/.tamagui": true,
    "**/.eslintrc.js": true,
    "**/next-env.d.ts": true,
    "**/.vscode": true,
    "**/.turbo": true,
    "**/out": true,
    "SECURITY.md": true,
    "LICENSE": true,
    ".gitpod.yml": true,
    ".npmrc": true,
    "pnpm-workspace.yaml": true,
    "renovate.json": true,
    "turbo.json": true
  },
  "search.exclude": {
    "**/out": true
  },
  "explorerExclude.backup": {},
  "apc.activityBar": {
    "position": "bottom",
    "hideSettings": true,
    "size": 20
  },
  "apc.statusBar": {
    "position": "editor-bottom",
    "height": 22,
    "fontSize": 12
  },
  "apc.electron": {
    "frame": false,
    "transparent": true,
    "vibrancy": "ultra-dark",
    "opacity": 0.9, // Range: 0-1 (0 means full transparency). If set, you won't see the element.
    "visualEffectState": "active",
    "titleBarStyle": "hiddenInset",
    "trafficLightPosition": {
      "x": 8,
      "y": 10
    }
  },
  "apc.header": {
    "height": 34,
    "fontSize": 14
  },
  "apc.listRow": {
    "height": 21,
    "fontSize": 13
  },
  "typescript.tsdk": "node_modules/typescript/lib"
  // Turn this on if you want to debug the T4 App Tools Extension
  // "terminal.integrated.defaultProfile.windows": null
}
